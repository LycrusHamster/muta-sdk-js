{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "traceResolution": false,
    "listEmittedFiles": false,
    "listFiles": false,
    "pretty": true,
    "composite": true,
    "lib": ["es2017", "dom"],
    "baseUrl": "./packages",
    "paths": {
      "@muta/shared": ["shared/src"],
      "@muta/types": ["muta-types/src"],
      "@muta/utils": ["packages/muta-utils/src"],
      "@muta/account": ["muta-account/src"],
      "@muta/client-raw": ["muta-client-raw/src"],
      "@muta/client": ["muta-client/src"],
      "@muta/service": ["muta-service/src"]
    }
  },
  "files": [],
  "references": []
}
